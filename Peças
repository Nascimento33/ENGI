    <div class="container">
        <!-- Espaço para a imagem do carrinho -->
        <div class="cart-image">
            <img src="img tr/carrinho.jfif" alt="Carrinho de compras">
        </div>

        <h2>Pagamento</h2>

        <form id="paymentForm">
            <div class="form-group">
                <label for="name">Nome Completo:</label>
                <input type="text" id="name" required>
                <span class="error" id="nameError">Por favor, insira o nome completo.</span>
            </div>

            <div class="form-group">
                <label for="paymentMethod">Forma de Pagamento:</label>
                <select id="paymentMethod" required>
                    <option value="">Selecione</option>
                    <option value="cartao-credito">Cartão de Crédito</option>
                    <option value="boleto">Boleto Bancário</option>
                    <option value="pix">PIX</option>
                    <option value="paypal">PayPal</option>
                </select>
                <span class="error" id="paymentMethodError">Por favor, selecione uma forma de pagamento.</span>
            </div>

            <div class="form-group" id="cardInfo" style="display:none;">
                <label for="card">Número do Cartão:</label>
                <input type="text" id="card">
                <span class="error" id="cardError">Por favor, insira um número de cartão válido.</span>
            </div>

            <div class="form-group" id="expiryInfo" style="display:none;">
                <label for="expiry">Data de Validade:</label>
                <input type="month" id="expiry">
                <span class="error" id="expiryError">Por favor, insira a data de validade.</span>
            </div>

            <div class="form-group" id="cvvInfo" style="display:none;">
                <label for="cvv">CVV:</label>
                <input type="text" id="cvv" maxlength="3">
                <span class="error" id="cvvError">Por favor, insira um CVV válido.</span>
            </div>

            <div class="form-group">
                <input type="submit" value="Pagar">
            </div>

            <div class="success" id="successMessage">Pagamento realizado com sucesso!</div>
        </form>
    </div>

    <script>
        const paymentForm = document.getElementById('paymentForm');
        const paymentMethod = document.getElementById('paymentMethod');
        const cardInfo = document.getElementById('cardInfo');
        const expiryInfo = document.getElementById('expiryInfo');
        const cvvInfo = document.getElementById('cvvInfo');

        paymentMethod.addEventListener('change', function() {
            const method = paymentMethod.value;
            if (method === 'cartao-credito') {
                cardInfo.style.display = 'block';
                expiryInfo.style.display = 'block';
                cvvInfo.style.display = 'block';
            } else {
                cardInfo.style.display = 'none';
                expiryInfo.style.display = 'none';
                cvvInfo.style.display = 'none';
            }
        });

        paymentForm.addEventListener('submit', function(event) {
            event.preventDefault();

            // Limpar mensagens de erro
            document.getElementById('nameError').style.display = 'none';
            document.getElementById('paymentMethodError').style.display = 'none';
            document.getElementById('cardError').style.display = 'none';
            document.getElementById('expiryError').style.display = 'none';
            document.getElementById('cvvError').style.display = 'none';

            let valid = true;

            // Validação do nome
            const name = document.getElementById('name').value;
            if (name.trim() === "") {
                document.getElementById('nameError').style.display = 'block';
                valid = false;
            }

            // Validação da forma de pagamento
            if (paymentMethod.value === "") {
                document.getElementById('paymentMethodError').style.display = 'block';
                valid = false;
            }

            // Se for cartão de crédito, validar os campos adicionais
            if (paymentMethod.value === 'cartao-credito') {
                const card = document.getElementById('card').value;
                const expiry = document.getElementById('expiry').value;
                const cvv = document.getElementById('cvv').value;

                if (card.trim() === "") {
                    document.getElementById('cardError').style.display = 'block';
                    valid = false;
                }

                if (expiry.trim() === "") {
                    document.getElementById('expiryError').style.display = 'block';
                    valid = false;
                }

                if (cvv.trim() === "" || cvv.length !== 3) {
                    document.getElementById('cvvError').style.display = 'block';
                    valid = false;
                }
            }

            if (valid) {
                document.getElementById('successMessage').style.display = 'block';
            }
        });
    </script>

